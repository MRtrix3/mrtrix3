mkdir -p tmp-dwi && mkdir -p tmp-mask && mrconvert BIDS/sub-02/dwi/sub-02_dwi.nii.gz -fslgrad BIDS/sub-02/dwi/sub-02_dwi.bvec BIDS/sub-02/dwi/sub-02_dwi.bval tmp-dwi/sub-02.mif && mrconvert BIDS/sub-02/dwi/sub-02_brainmask.nii.gz tmp-mask/sub-02.mif && mrconvert BIDS/sub-03/dwi/sub-03_dwi.nii.gz -fslgrad BIDS/sub-03/dwi/sub-03_dwi.bvec BIDS/sub-03/dwi/sub-03_dwi.bval tmp-dwi/sub-03.mif && mrconvert BIDS/sub-03/dwi/sub-03_brainmask.nii.gz tmp-mask/sub-03.mif && dwiintensitynorm tmp-dwi/ tmp-mask/ tmp-out/ tmp1.mif tmp2.mif -force && testing_diff_image tmp-out/sub-02.mif dwiintensitynorm/sub-02.mif.gz -frac 1e-3 && testing_diff_image tmp-out/sub-03.mif dwiintensitynorm/sub-03.mif.gz -frac 1e-3 && testing_diff_image tmp1.mif dwiintensitynorm/fa.mif.gz -abs 1e-3 && testing_diff_image tmp2.mif dwiintensitynorm/mask.mif.gz
