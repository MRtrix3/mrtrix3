dwi2tensor dwi.mif - | testing_diff_data - dwi2tensor/out.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif - | testing_diff_data - dwi2tensor/out_mask.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -b0 - tmp_dt.mif | testing_diff_data - dwi2tensor/out_b0.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -iter 0 - | testing_diff_data - dwi2tensor/out_it0.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -iter 1 - | testing_diff_data - dwi2tensor/out_it1.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -iter 2 - | testing_diff_data - dwi2tensor/out_it2.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -iter 3 - | testing_diff_data - dwi2tensor/out_it3.mif 1e-12
dwi2tensor dwi.mif -mask mask.mif -iter 4 - | testing_diff_data - dwi2tensor/out_it4.mif 1e-12
