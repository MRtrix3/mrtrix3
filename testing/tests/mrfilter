mrfilter dwi.mif smooth - | testing_diff_data - mrfilter/out1.mif -frac 1e-5
mrfilter dwi.mif smooth -stdev 1.5,1.5,1.5 - | testing_diff_data - mrfilter/out2.mif -frac 1e-5
mrfilter dwi.mif smooth -stdev 1.4 - | testing_diff_data - mrfilter/out3.mif -frac 1e-5
mrfilter dwi.mif smooth -fwhm 3,3,3 - | testing_diff_data - mrfilter/out4.mif -frac 1e-5
mrfilter dwi.mif smooth -stdev 1.4 -extent 5 - | testing_diff_data - mrfilter/out5.mif -frac 1e-5
mrfilter dwi.mif median - | testing_diff_data - mrfilter/out6.mif -frac 1e-5
mrfilter dwi.mif median -extent 5,3,1 - | testing_diff_data - mrfilter/out7.mif -frac 1e-5
mrfilter dwi.mif median -extent 5 - | testing_diff_data - mrfilter/out8.mif -frac 1e-5
