#!/bin/bash
# Verify basic operation of panchromatic sharpening, 
#   where a grayscale anatomical image is provided using the -contrast option
# Ensure that output matches that generated by a prior software version
fod2dec fod2dec/fod.mif - -contrast fod2dec/anat.mif | \
testing_diff_image - fod2dec/out/dec-anat.mif -voxel 1e-5
