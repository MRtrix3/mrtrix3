#!/bin/bash
# Evaluation of command under default operation
#   with no modification of behaviour using command-line options
#   (do however export all possible fixel-wise quantitative metrics)
# Ensure that output matches that generated by a prior software version
rm -rf tmp/

fod2fixel -nthreads 1 fod.mif tmp/ \
-afd afd.mif \
-peak peak.mif \
-disp disp.mif \
-skew skew.mif \
-force

testing_diff_fixel -frac 10e-5 tmp/ fod2fixel/out1
