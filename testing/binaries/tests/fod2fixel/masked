#!/bin/bash
# Verify operation when a user-specified voxel mask is provided
# Ensure that output matches that generated by a prior software version
rm -rf tmp/

fod2fixel -nthreads 1 fod.mif tmp/ \
-mask mask.mif \
-afd afd.mif \
-peak peak.mif \
-disp disp.mif \
-skew skew.mif \
-force

testing_diff_fixel -frac 10e-5 tmp/ fod2fixel/out2
